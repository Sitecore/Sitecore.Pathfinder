<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <hooks>
            <hook type="Sitecore.Pathfinder.Hooks.InitializeSitecorePathfinder, Sitecore.Pathfinder.Server" />
        </hooks>

        <pathfinder>
            <nuget-repositories>
                <repository url="https://api.nuget.org/v3/index.json" />
            </nuget-repositories>
        </pathfinder>
    </sitecore>
</configuration>
