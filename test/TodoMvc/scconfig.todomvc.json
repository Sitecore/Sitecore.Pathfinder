{
    "project-role": "speak",

    "project-website-mappings": {
        "project-to-website": {
            "todomvc": {
                "database" : "core",
                "project-directory-to-item-path": "/items/core/sitecore/client/Applications/TodoMvc => /sitecore/client/Applications/TodoMvc",
                "project-directory-to-website-directory": "/items/core/sitecore/client/Applications/TodoMvc => /sitecore/shell/client/Applications/TodoMvc"
            }
        },

        "website-to-project": {
            "todomvc": {
                "database": "core",
                "item-path-to-project-directory": "/sitecore/client/Applications/TodoMvc => /items/core/sitecore/client/Applications/TodoMvc",
                "website-directory-to-project-directory": "/sitecore/shell/client/Applications/TodoMvc => /items/core/sitecore/client/Applications/TodoMvc"
            }
        }
    },
    
    "dependencies": {
        "sitecore-business-component-library": { "id": "Sitecore.BusinessComponentLibrary", "version": "1.0.0" },
        "sitecore-core": { "id": "Sitecore.Core", "version": "1.0.0" },
        "sitecore-launchpad": { "id": "Sitecore.Launchpad", "version": "1.0.0" },
        "sitecore-master": { "id": "Sitecore.Master", "version": "1.0.0" },
        "sitecore-speak": { "id": "Sitecore.Speak", "version": "1.0.0" }
    },  

    "show-website": {
        "start-url": "/sitecore/client/Applications/TodoMvc/content/TodoMvc"
    }
}
